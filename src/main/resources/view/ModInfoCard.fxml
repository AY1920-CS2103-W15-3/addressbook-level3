<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<StackPane fx:id="placeHolder" prefWidth="500.0" styleClass="pane-with-border" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <VBox>
      <children>
         <Label prefHeight="30.0" text="\$modCode">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
         </Label>
         <Label text="\$modNote" />
         <VBox>
            <children>
               <StackPane fx:id="modLinksPanelPlaceholder" prefHeight="150.0" prefWidth="500.0" />
            </children>
         </VBox>
      </children>
   </VBox>
</StackPane>
